  ..\..\appssi.obj ..\..\..\misc\wrdstrms.obj ..\..\..\misc\timeutil.obj \
  ..\..\..\misc\base64.obj ..\..\..\misc\virttext.obj ..\..\..\misc\cookutil.obj \
  ..\..\..\misc\inetstr.obj ..\..\browscap.obj ..\..\..\misc\sds.obj \
  ..\..\ipcrqst.obj ..\..\..\misc\ipc.obj ..\..\httpreq.obj \
  ..\..\..\misc\d3sysutl.obj ..\..\wvardict.obj ..\..\wipcshrd.obj \
  ..\..\adrotate.obj ..\..\..\misc\xstrings.obj ..\..\waptxrdr.obj \
  ..\..\wapschdl.obj ..\..\wapwthrd.obj ..\..\wapactns.obj ..\..\wapmask.obj \
  ..\..\WapRTTI.obj ..\..\..\misc\MthdsLst.obj ..\..\wapthntf.obj ..\..\wcp.obj \
  ..\..\..\misc\comcat.obj ..\..\IpcConv.obj hwap10.obj
RESFILES = hwap10.res ..\..\hwebapp.dcr ..\..\waptmplt.dcr ..\..\wscr_tlb.dcr \
  ..\..\htmltxt.dcr ..\..\browscap.dcr ..\..\adrotate.dcr
DEFFILE =
RESDEPEN = $(RESFILES)
LIBFILES =
LIBRARIES =
SPARELIBS =
PACKAGES = vcl35.bpi
# ---------------------------------------------------------------------------
PATHCPP = .;
PATHASM = .;..\..;..\..\..\misc
PATHPAS = .;
PATHRC = .;
DEBUGLIBPATH = $(BCB)\lib\debug
RELEASELIBPATH = $(BCB)\lib\release
# ---------------------------------------------------------------------------
CFLAG1 = -Od -Hc -w -Ve -r- -k -y -v -vi- -c -b- -w-par -w-inl -Vx
CFLAG2 = -I..\..\..\webapp;..\..\..\misc;$(BCB)\include;$(BCB)\include\vcl \
  -D_RTLDLL;USEPACKAGES -H=$(BCB)\lib\vcl35.csm
CFLAG3 = -Tkh30000
PFLAGS = -U..\..\..\webapp;..\..\..\misc;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -I..\..\..\webapp;..\..\..\misc;$(BCB)\include;$(BCB)\include\vcl \
  -D_RTLDLL;USEPACKAGES -$Y -$W -$O- -v -JPHN -M
RFLAGS = -i..\..\..\webapp;..\..\..\misc;$(BCB)\include;$(BCB)\include\vcl \
  -D_RTLDLL;USEPACKAGES
AFLAGS = /i$(BCB)\include /i$(BCB)\include\vcl /d_RTLDLL /dUSEPACKAGES /mx /w2 /zd
LFLAGS = -L..\..\..\webapp;..\..\..\misc;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -D"HyperAct WebApp Runtime Package" -aa -Tpp -x -Gn -Gl -Gi -v
IFLAGS =
# ---------------------------------------------------------------------------
ALLOBJ = c0pkg32.obj $(PACKAGES) sysinit.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES) $(LIBRARIES) import32.lib cp32mti.lib
